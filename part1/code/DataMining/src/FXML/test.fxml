<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="600.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.StaticController">
  <children>
    <MenuBar VBox.vgrow="NEVER">
      <menus>
        <Menu mnemonicParsing="false" text="Choose">
          <items>
            <MenuItem mnemonicParsing="false" onAction="#buildXYChart" text="XY Chart" />
                  <MenuItem mnemonicParsing="false" onAction="#buildPieChart" text="Pie Chart" />
                  <MenuItem mnemonicParsing="false" onAction="#buildAreaChart" text="Area Chart" />
          </items>
        </Menu>
      </menus>
    </MenuBar>
      <AnchorPane fx:id="chartArea" />
  </children>
</VBox>
